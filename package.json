{
  "name": "@xqmsg/jssdk-core",
  "version": "2.0.2",
  "description": "A Javascript Implementation of XQ Message SDK, V.2",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/browser/jssdk-core.min.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "directories": {
    "test": "__tests__"
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "build": "concurrently \"webpack --config webpack.config.js\" \"tsc -p tsconfig-esm.json\" \"tsc -p tsconfig-cjs.json\"",
    "lint": "eslint src/**  --ext .ts",
    "clean": "rm -rf node_modules && rm -rf dist",
    "test": "jest",
    "npm-publish": "yarn build && npm publish --access public",
    "yalc-publish": "yarn build && yalc publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XQ-Message-Inc/jssdk-core.git"
  },
  "keywords": [
    "security",
    "zero trust",
    "plug and play",
    "encryption",
    "AES",
    "XOR",
    "privacy",
    "quantum",
    "protection",
    "cyber",
    "cybersecurity"
  ],
  "author": "XQ Message",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/XQ-Message-Inc/jssdk-core/issues"
  },
  "homepage": "https://github.com/XQ-Message-Inc/jssdk-core#readme",
  "devDependencies": {
    "@types/crypto-js": "^4.0.1",
    "@types/jest": "^26.0.23",
    "@types/memory-cache": "^0.2.1",
    "@types/node": "^15.6.0",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "concurrently": "^6.2.0",
    "eslint": "7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.1",
    "path-browserify": "^1.0.1",
    "prettier": "^2.3.0",
    "source-map-loader": "^3.0.0",
    "text-encoding": "^0.7.0",
    "ts-jest": "^27.0.1",
    "ts-loader": "^9.2.2",
    "typescript": "^4.2.4",
    "webpack": "^5.5.1",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "crypto-js": "3.1.9-1",
    "jwt-decode": "^3.1.2",
    "memory-cache": "^0.2.0",
    "xhr2": "^0.2.1",
    "yalc": "^1.0.0-pre.53"
  }
}
